{
  "name": "esfinder",
  "type": "module",
  "version": "0.0.2-alpha.5",
  "description": "A fast and lightweight tool for analyzing module dependencies and finding which files depend on a given module.",
  "author": "Sunny-117",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Sunny-117",
  "homepage": "https://github.com/Sunny-117/esfinder#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sunny-117/esfinder.git"
  },
  "bugs": "https://github.com/Sunny-117/esfinder/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./swc": "./dist/swc.mjs",
    "./oxc": "./dist/oxc.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "esfinder": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "dev": "unbuild --stub",
    "example": "nodemon",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@babel/parser": "^7.26.8",
    "@babel/traverse": "^7.26.8",
    "@swc/core": "^1.10.18",
    "commander": "^14.0.2",
    "globby": "^14.1.0",
    "oxc-parser": "^0.96.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.1.1",
    "@antfu/ni": "^23.3.1",
    "@antfu/utils": "^8.1.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^22.13.1",
    "bumpp": "^10.0.2",
    "eslint": "^9.20.0",
    "lint-staged": "^15.4.3",
    "pnpm": "^10.2.1",
    "simple-git-hooks": "^2.11.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "unbuild": "^3.3.1",
    "vite": "^6.1.0",
    "vitest": "^3.0.5"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
