{
  "name": "esfinder",
  "type": "module",
  "version": "0.0.2-alpha.7",
  "description": "A fast and lightweight tool for analyzing module dependencies and finding which files depend on a given module.",
  "author": "Sunny-117",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Sunny-117",
  "homepage": "https://github.com/Sunny-117/esfinder#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Sunny-117/esfinder.git"
  },
  "bugs": "https://github.com/Sunny-117/esfinder/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": "./dist/index.mjs",
    "./swc": "./dist/swc.mjs",
    "./oxc": "./dist/oxc.mjs"
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "bin": {
    "esfinder": "./dist/cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "robuild",
    "dev": "robuild --watch",
    "example": "nodemon",
    "lint": "eslint .",
    "prepublishOnly": "nr build",
    "release": "bumpp && pnpm publish",
    "start": "tsx src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "prepare": "simple-git-hooks"
  },
  "dependencies": {
    "@babel/parser": "^7.29.0",
    "@babel/traverse": "^7.29.0",
    "@swc/core": "^1.15.11",
    "commander": "^14.0.3",
    "globby": "^16.1.0",
    "oxc-parser": "^0.114.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^7.4.3",
    "@antfu/ni": "^28.2.0",
    "@antfu/utils": "^9.3.0",
    "@types/babel__traverse": "^7.28.0",
    "@types/node": "^25.2.3",
    "bumpp": "^10.4.1",
    "eslint": "^10.0.0",
    "lint-staged": "^16.2.7",
    "pnpm": "^10.29.3",
    "robuild": "^0.1.3",
    "simple-git-hooks": "^2.13.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
